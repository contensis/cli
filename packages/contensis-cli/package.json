{
  "name": "contensis-cli",
  "version": "1.3.1-beta.6",
  "description": "A fully featured Contensis command line interface with a shell UI provides simple and intuitive ways to manage or profile your content in any NodeJS terminal.",
  "repository": "https://github.com/contensis/cli",
  "homepage": "https://github.com/contensis/cli/tree/main/packages/contensis-cli#readme",
  "main": "dist/index.js",
  "bin": {
    "contensis": "./cli.js",
    "contensis-cli": "./cli.js"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "npm run cli",
    "dev:shell": "npm run prestart && nodemon --watch dist --inspect=9229 ./dist/shell.js",
    "prebuild": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "build": "node esbuild.config.js",
    "build:watch": "node esbuild.config.js --watch",
    "cli": "node --inspect=9229 ./cli.js",
    "postinstall": "npx patch-package",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package-lock": "npm i --package-lock-only --workspaces false"
  },
  "author": "Zengenti",
  "license": "ISC",
  "dependencies": {
    "@action-validator/core": "^0.5.3",
    "adm-zip": "^0.5.10",
    "app-root-path": "^3.1.0",
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "csv": "^6.1.0",
    "dayjs": "^1.11.6",
    "diff": "^5.1.0",
    "figlet": "^1.5.2",
    "flat": "^5.0.2",
    "giturl": "^2.0.0",
    "hosted-git-info": "^6.1.1",
    "inquirer-command-prompt": "^0.1.0",
    "jsonpath-mapper": "^1.1.0",
    "keytar": "^7.9.0",
    "lodash": "^4.17.21",
    "migratortron": "^1.0.0-beta.64",
    "nanospinner": "^1.1.0",
    "node-fetch": "^2.6.7",
    "parse-git-config": "^3.0.0",
    "patch-package": "^6.4.7",
    "printable-characters": "^1.0.42",
    "split-cmd": "^1.1.0",
    "xml2js": "^0.4.23",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@octokit/types": "^12.4.0",
    "@types/adm-zip": "^0.5.5",
    "@types/figlet": "^1.5.4",
    "@types/flat": "^5.0.2",
    "@types/hosted-git-info": "^3.0.2",
    "@types/inquirer": "^8.2.1",
    "@types/json2csv": "^5.0.3",
    "@types/lodash": "^4.14.182",
    "@types/node-fetch": "^2.6.1",
    "@types/parse-git-config": "^3.0.1",
    "@types/xml2js": "^0.4.11",
    "esbuild": "^0.24.0",
    "esbuild-node-externals": "^1.15.0",
    "esbuild-plugin-glob": "^2.2.3",
    "nodemon": "^2.0.18",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.6.9",
    "typescript": "^5.6.3"
  }
}
