{
  "name": "contensis-cli",
  "version": "1.0.0-beta.1",
  "description": "A fully featured Contensis command line interface with a shell UI provides simple and intuitive ways to manage or profile your content in any NodeJS terminal.",
  "repository": "https://github.com/contensis/node-cli",
  "homepage": "https://github.com/contensis/node-cli/tree/main/packages/contensis-cli#readme",
  "main": "dist/index.js",
  "bin": {
    "contensis": "./shell.js",
    "contensis-cli": "./cli.js",
    "contensis-cli-headless": "./headless-setup.sh"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "npm run cli",
    "start:shell": "npm run prestart && npm run shell",
    "dev:shell": "npm run prestart && nodemon --watch dist --inspect=9229 ./dist/shell.js",
    "prebuild": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "build": "node esbuild.config.js",
    "build:watch": "node esbuild.config.js --watch",
    "cli": "node ./dist/index.js",
    "shell": "node ./shell.js",
    "postinstall": "patch-package",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Zengenti",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.3.0",
    "csv": "^6.1.0",
    "figlet": "^1.5.2",
    "flat": "^5.0.2",
    "inquirer-command-prompt": "^0.1.0",
    "json2csv": "^5.0.7",
    "keytar": "^7.9.0",
    "lodash": "^4.17.21",
    "migratortron": "^1.0.0-beta.11",
    "node-fetch": "^2.6.7",
    "patch-package": "^6.4.7",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.4",
    "@types/flat": "^5.0.2",
    "@types/inquirer": "^8.2.1",
    "@types/json2csv": "^5.0.3",
    "@types/lodash": "^4.14.182",
    "@types/node-fetch": "^2.6.1",
    "@types/xml2js": "^0.4.11",
    "esbuild": "^0.14.43",
    "esbuild-node-externals": "^1.4.1",
    "esbuild-plugin-glob": "^1.1.2",
    "nodemon": "^2.0.18",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.6.9",
    "typescript": "^4.7.3"
  }
}
