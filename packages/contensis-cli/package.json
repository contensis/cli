{
  "name": "contensis-cli",
  "version": "1.5.1-beta.8",
  "description": "A fully featured Contensis command line interface with a shell UI provides simple and intuitive ways to manage or profile your content in any NodeJS terminal.",
  "repository": "https://github.com/contensis/cli",
  "homepage": "https://github.com/contensis/cli/tree/main/packages/contensis-cli#readme",
  "main": "dist/index.js",
  "bin": {
    "contensis": "./cli.js",
    "contensis-cli": "./cli.js"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "npm run cli",
    "dev:shell": "npm run prestart && nodemon --watch dist --inspect=9229 ./dist/shell.js",
    "prebuild": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "build": "node esbuild.config.js",
    "build:watch": "node esbuild.config.js --watch",
    "cli": "node --inspect=9229 ./cli.js",
    "patches": "npx patch-package --error-on-fail",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package-lock": "npm i --package-lock-only --workspaces false"
  },
  "author": "Zengenti",
  "license": "ISC",
  "dependencies": {
    "@action-validator/core": "^0.6.0",
    "adm-zip": "^0.5.16",
    "app-root-path": "^3.1.0",
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "csv": "^6.3.11",
    "dayjs": "^1.11.13",
    "diff": "^7.0.0",
    "figlet": "^1.8.0",
    "flat": "^5.0.2",
    "giturl": "^2.0.0",
    "hosted-git-info": "^8.0.2",
    "inquirer-command-prompt": "^0.1.0",
    "keytar": "^7.9.0",
    "lodash": "^4.17.21",
    "migratortron": "^1.0.0-beta.79",
    "nanospinner": "^1.2.0",
    "node-fetch": "^2.6.7",
    "parse-git-config": "^3.0.0",
    "patch-package": "^8.0.0",
    "printable-characters": "^1.0.42",
    "split-cmd": "^1.1.0",
    "xml2js": "^0.6.2",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@octokit/types": "^13.6.2",
    "@types/adm-zip": "^0.5.7",
    "@types/figlet": "^1.7.0",
    "@types/flat": "^5.0.5",
    "@types/hosted-git-info": "^3.0.5",
    "@types/inquirer": "^9.0.7",
    "@types/json2csv": "^5.0.7",
    "@types/lodash": "^4.17.13",
    "@types/node-fetch": "^2.6.12",
    "@types/parse-git-config": "^3.0.4",
    "@types/xml2js": "^0.4.14",
    "esbuild": "^0.24.0",
    "esbuild-node-externals": "^1.15.0",
    "esbuild-plugin-glob": "^2.2.3",
    "nodemon": "^2.0.18",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2"
  }
}
